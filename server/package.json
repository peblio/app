{
  "name": "peblio-server",
  "version": "1.0.0",
  "description": "",
  "main": ".build/server.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "build": "./node_modules/.bin/babel src --out-dir .build --source-maps",
    "prestartserver": "./node_modules/.bin/babel src --out-dir .build",
    "startserver": "node .build/server.js",
    "prebuild": "rm -rf .build/*",
    "prestart": "npm run build",
    "start": "AWS_PROFILE=peblio ./scripts/run_with_credstash.sh nodemon .build/server.js",
    "test": "./node_modules/mocha/bin/mocha --recursive --exit --compilers js:babel-core/register --require babel-polyfill test",
    "integrationTest": "AWS_PROFILE=peblio ENVIRONMENT=test ./scripts/run_with_credstash.sh ./node_modules/mocha/bin/mocha --recursive --exit --compilers js:babel-core/register --require babel-polyfill integrationTest",
    "allTest": "npm test && npm run integrationTest"
  },
